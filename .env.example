# Fireup Development Environment Configuration

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5433
POSTGRES_DB=fireup_dev
POSTGRES_USER=fireup
POSTGRES_PASSWORD=fireup_dev_password
DATABASE_URL=postgresql://fireup:fireup_dev_password@localhost:5433/fireup_dev

# Logging Configuration
RUST_LOG=fireup=debug,info
RUST_BACKTRACE=1

# Application Configuration
FIREUP_MAX_BATCH_SIZE=1000
FIREUP_CONNECTION_POOL_SIZE=10
FIREUP_IMPORT_TIMEOUT_SECONDS=3600