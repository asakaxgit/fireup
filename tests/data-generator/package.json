{
  "name": "data-generator",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "emulator": "npx firebase emulators:start --import=../.firestore-data --export-on-exit",
    "emulator-ui": "npx firebase emulators:start --import=../.firestore-data --export-on-exit",
    "build": "tsc",
    "generate": "tsc && chmod +x dist/run-generator.js && npx firebase emulators:exec --export-on-exit=../.firestore-data 'node dist/run-generator.js'",
    "generate:run": "tsc && chmod +x dist/run-generator.js && npx firebase emulators:exec --export-on-exit=../.firestore-data",
    "generate:no-emulator": "tsc && FIRESTORE_EMULATOR_HOST=localhost:18080 node dist/run-generator.js",
    "dev": "npx firebase emulators:start --import=../.firestore-data --export-on-exit",
    "open-ui": "open http://localhost:14000/firestore/default/data"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.9.2",
    "firebase-tools": "^14.23.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "firebase": "^12.5.0",
    "firebase-admin": "^13.5.0"
  }
}